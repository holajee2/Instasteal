-- ===== YK INSTA STEAL LOADER =====

-- ğŸ”‘ KEY DEL USUARIO
local KEY = getgenv().KEY or ""

if KEY == "" then
    warn("âŒ No key detectada")
    return
end

-- ğŸ” KEYATH AUTH
local AUTH_URL = "https://keyath.xyz/api/verify"

local response
pcall(function()
    response = game:HttpGet(AUTH_URL .. "?key=" .. KEY)
end)

-- âŒ FALLA
if response ~= "valid" then
    warn("âŒ Key invÃ¡lida / expirada / HWID incorrecto")
    return
end

-- âœ… AUTORIZADO
_G.__AUTH_OK = true

-- ğŸš€ SCRIPT PRINCIPAL (OBFUSCADO)
loadstring(game:HttpGet(
    "https://raw.githubusercontent.com/holajee2/Instasteal/main/main.lua"
